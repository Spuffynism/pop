{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "pop",
  "title": "Pop",
  "description": "Pop open a project, wherever it is.",
  "icon": "command-icon.png",
  "author": "puff",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "pop",
      "title": "Pop",
      "description": "Pop open a project",
      "mode": "no-view",
      "arguments": [
        {
          "name": "project",
          "placeholder": "Project",
          "type": "text",
          "required": true
        },
        {
          "name": "branch",
          "placeholder": "Branch",
          "type": "text"
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.42.1",
    "@raycast/utils": "^1.4.10",
    "execa": "^6.1.0",
    "shell-env": "^4.0.1"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
